<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".animal_category_Activity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="5dp"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/choose_anm"
            android:textStyle="bold"
            android:textColor="#066006"
            android:layout_marginLeft="10dp"
            android:fontFamily="@font/montserrat_bold"
            android:textSize="50dp"/>
        <FrameLayout
            android:layout_width="match_parent"
            android:background="@color/frame"
            android:layout_below="@+id/hh"
            android:layout_margin="12dp"
            android:layout_height="2dp"/>
<!--      cow-->
        <com.google.android.material.card.MaterialCardView
            android:layout_width="match_parent"
            android:id="@+id/cow"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:layout_gravity="center_horizontal"
            android:checkable="true"
            app:cardBackgroundColor="@android:color/white"
            app:cardForegroundColor="@android:color/transparent"
            app:checkedIconSize="0dp"
            app:cardElevation="2dp"
            app:strokeWidth="4dp"
            app:strokeColor="@color/effect"
            app:cardCornerRadius="20dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">



                <com.google.android.material.card.MaterialCardView
                    android:layout_width="180dp"
                    android:layout_height="180dp"
                    android:layout_gravity="center_vertical"
                    android:checkable="true"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="5dp"
                    app:cardBackgroundColor="@android:color/white"
                    app:cardForegroundColor="@android:color/transparent"
                    app:strokeColor="@color/newww"
                    app:cardCornerRadius="20dp">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="180dp"

                        >

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:scaleType="centerCrop"
                            android:src="@drawable/cow" />


                    </RelativeLayout>

                </com.google.android.material.card.MaterialCardView>
                <FrameLayout
                    android:layout_width="2dp"
                    android:background="@color/frame"
                    android:layout_below="@+id/hh"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_height="180dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cow"
                    android:textStyle="bold"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/montserrat"
                    android:textColor="@color/black"
                    android:textSize="30dp"/>
            </LinearLayout>
        </com.google.android.material.card.MaterialCardView>

